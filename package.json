{
  "name": "bookmark-json-converter",
  "version": "1.0.1",
  "description": "A TypeScript library for converting browser bookmark files to/from JSON",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "bookmark",
    "json",
    "converter",
    "parser",
    "serializer"
  ],
  "author": "ReinforceZwei",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "vitest": "^2.0.0",
    "@vitest/ui": "^2.0.0"
  }
}
